<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
        <class name="xin.qiliuhai.domain.downStation" table="downstation">
        <!--主键对应关系-->
            <id name="Id" column="id">
            	<!--主键生成策略  -->
                <generator class="native"/>
            </id>
            <property name="deltat" column="deltat"/>
            <property name="distance" column="distance"/>
            <property name="downStationId" column="downStationId"/>
             <property name="riverId" column="riverId"/>
             <property name="stationId" column="stationId"/>
            <!-- <set name="linkman" inverse="true" >
            	<key column="lkm_cust_id" />
            	<one-to-many class="com.itcast.domain.Linkman" />
            </set> -->
        </class>
  </hibernate-mapping>