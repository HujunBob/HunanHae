		<property name="floodMaxZ" column="floodmaxz" />
		<property name="guaranteedN" column="guaranteedn" />
		<property name="inUse" column="inuse" />
		<property name="KValue" column="kvalue" />
		<property name="LongCanUse" column="longcanuse" />
		<property name="MiddleCanUse" column="middlecanuse" />
		<property name="nariStationId" column="naristationid" />
		<property name="normalZ" column="normalZ" />
		<property name="plantsMaxQ" column="plantsmaxq" />
		<property name="plantsNum" column="plantsnum" />
		<property name="plantsTypeNum" column="plantsTypeNum" />
		<property name="ratedN" column="ratedn" />
		<property name="relativeX" column="relativex" />
		<property name="relativeY" column="relativey" />
		<property name="riverId" column="riverid" />
		<property name="shortCanUse" column="shortcanuse" />
		<property name="stationMaxR" column="stationmaxr" />
		<property name="stationMinR" column="stationminr" />
		<property name="stationName" column="StationName" />
		<property name="totalV" column="totalv" />
		<!-- <set name="linkman" inverse="true" > <key column="lkm_cust_id" /> 
			<one-to-many class="com.itcast.domain.Linkman" /> </set> -->
	</class>
</hibernate-mapping>