<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="xin.qiliuhai.domain.stationInfo" table="stationinfo">
		<!--主键对应关系 -->
		<id name="stationId" column="stationid">
			<!--主键生成策略 -->
			<generator class="native" />
		</id>
		<property name="activeV" column="activeV" />
		<property name="adjustType" column="adjusttype" />
		<property name="checkFloodZ" column="checkfloodz" />
		<property name="deadV" column="deadv" />
		<property name="deadZ" column="deadz   " />
		<property name="designFloodZ" column="designfloodz" />
		<property name="fittingCanUse" column="fittingCanUse" />
